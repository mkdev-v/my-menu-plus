{
  "manifest_version": 3,
  "name": "MyMenu+",
  "version": "1.0",
  "description": "Easily manage and access your favorite URLs from the right-click menu.",
  "icons": {
    "48": "icon48.png"
  },
  "permissions": [
    "contextMenus",
    "tabs",
    "storage"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "action": {
    "default_title": "MyMenu+",
    "default_icon": "icon48.png"
  }
}